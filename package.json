{
  "name": "flashcards",
  "version": "0.1.0",
  "description": "Flashcards web app",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "cypress:open": "cypress open",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate --schema=./lib/prisma/schema.prisma",
    "generate:nexus": "ts-node --transpile-only -P ./nexus.tsconfig.json ./lib/graphql/schema.ts",
    "graphql:codegen": "graphql-codegen --config codegen.yml",
    "ngrok": "ngrok http 3000"
  },
  "dependencies": {
    "@headlessui/react": "^1.1.1",
    "@heroicons/react": "^1.0.1",
    "@prisma/client": "^2.22.1",
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/forms": "^0.3.2",
    "@tailwindcss/line-clamp": "^0.2.0",
    "@tailwindcss/typography": "^0.4.0",
    "apollo-server-micro": "^2.24.0",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "graphql-scalars": "^1.9.3",
    "next": "^11.1.0",
    "next-auth": "^3.23.0",
    "nexus": "^1.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.2.0",
    "react-query": "^3.16.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.4",
    "@graphql-codegen/introspection": "^1.18.2",
    "@graphql-codegen/typescript": "^1.22.0",
    "@graphql-codegen/typescript-document-nodes": "^1.17.11",
    "@graphql-codegen/typescript-operations": "^1.17.16",
    "@graphql-codegen/typescript-react-query": "^1.3.0",
    "@types/graphql": "^14.5.0",
    "@types/next-auth": "^3.15.0",
    "@types/node": "^15.0.2",
    "@types/react": "^17.0.5",
    "autoprefixer": "^10.0.4",
    "cypress": "^7.2.0",
    "postcss": "^8.1.10",
    "prisma": "^2.22.1",
    "tailwindcss": "^2.1.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.5"
  },
  "prisma": {
    "schema": "lib/prisma/schema.prisma"
  }
}
